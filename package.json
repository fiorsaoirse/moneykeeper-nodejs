{
  "name": "src",
  "version": "1.0.0",
  "description": "Node.js based (koa) backend REST server for moneykeeper app",
  "main": "dist/start.ts",
  "scripts": {
    "test": "jest --runInBand",
    "watch": "nodemon --watch 'dist/**/*' -e ts,tsx --exec ts-node ./dist/bin/start.js",
    "build": "npm run build:ts & npm run build:types",
    "build:types": "tsc",
    "build:ts": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "npm run lint:src && npm run lint:ts",
    "lint:src": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "lint:src:fix": "npm run lint:src -- --fix",
    "lint:ts": "tsc --noEmit",
    "lint:ts:w": "npm run lint:ts -- --watch"
  },
  "keywords": [],
  "author": "Maria Kovsharova",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-cors": "0.0.16",
    "koa-joi-swagger-ts": "^1.2.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "lodash-es": "^4.17.21",
    "pg": "^8.5.1",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bluebird": "^3.5.33",
    "@types/jest": "^26.0.21",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cors": "0.0.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.35",
    "@types/pg": "^7.14.11",
    "@types/prettier": "^2.2.3",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  }
}